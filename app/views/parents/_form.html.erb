<%= simple_form_for(@parent) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :registration_id %>
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :role %>
    <%= f.input :email %>
    <%= f.input :volunteer %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
