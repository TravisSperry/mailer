<div class="container">
  <% title "Contact" %>

<p>
  <strong>First Name:</strong>
  <%= @contact.first_name %>
</p>
<p>
  <strong>Last Name:</strong>
  <%= @contact.last_name %>
</p>
<p>
  <strong>Email:</strong>
  <%= @contact.email %>
</p>

<p>
  <%= link_to "Edit", edit_contact_path(@contact) %> |
  <%= link_to "Destroy", @contact, :confirm => 'Are you sure?', :method => :delete %> |
  <%= link_to "View All", contacts_path %>
</p>
</div>